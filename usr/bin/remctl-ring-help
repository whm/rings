#!/bin/bash

echo "Valid commands:"
echo ""
echo "remctl ring load <ring id>"
echo ""
echo "    Request that pictures be loaded into the rings database"
echo ""
echo "remctl ring daemon <ring id>"
echo ""
echo "    Control the ring daemons"
echo ""
echo "remctl queue <ring id>"
echo ""
echo "    Show the status of the processing daemons"
echo ""
echo "remctl show"
echo ""
echo "    Display the ring IDs on this server"
echo ""
echo "remctl help"
echo ""
echo "    This display"
exit
